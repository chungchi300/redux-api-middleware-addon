{
  "name": "redux-api-middleware-addon",
  "version": "1.0.0",
  "title": "redux-api-middleware-addon",
  "main": "build/lib/index.min.js",
  "description":
    "A opinioned boilerplate for develop react component(with styled-components) and redux action,reducer with proper development tools",
  "keywords": [
    "Babel",
    "Browsersync",
    "React",
    "ES6",
    "TreeShaking",
    "Library",
    "Rollup"
  ],
  "homepage": "https://github.com/chungchi300/redux-api-middleware-addon",
  "author": {
    "name": "Jeff Chung",
