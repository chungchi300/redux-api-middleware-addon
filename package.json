  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chungchi300/redux-api-middleware-addon"
  },
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=6.0.0"
  },
  "dependencies": {
    "form-data": "^2.3.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "query-string": "^5.0.1",
    "redux-api-middleware": "^2.0.1",
    "rollup-plugin-json": "^2.3.0",
    "urijs": "^1.18.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "browser-sync": "2.18.12",
    "classnames": "^2.2.5",
    "eslint": "^3.3.1",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.33.0",
    "jest": "^18.1.0",
    "moment": "^2.17.1",
    "npm-run-all": "4.0.2",
    "opener": "^1.4.3",
    "prop-types": "^15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "^5.0.5",
    "react-test-renderer": "15.3.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rollup": "0.41.6",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "3.2.2",
    "styled-components": "^2.1.1",
    "ua-parser-js": "^0.7.13"
  },
  "scripts": {
    "lint": "eslint src --quiet --cache && echo \"eslint: no lint errors\"",
    "localAppBuildWatch:dev": "rollup -c make/app/dev.js -w",
    "localAppBuild:dev": "rollup -c make/app/dev.js",
    "prelocalAppStart": "rm -rf build/app/* && npm run localAppBuild:dev",
    "localAppStart": "npm-run-all --parallel localAppBuildWatch:dev browse",
    "browse":
      "browser-sync start --s --index 'html/index-dev.html' --files 'html/**/*.html, build/**/*.js' --no-notify",
    "prebuild": "rm -rf build/lib/*",
    "build": "rollup -c make/lib/prod.js",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "preversion": "git pull && npm up && npm run std --silent",
    "version": "npm run build && git add -A .",
    "postversion": "git push --tags origin HEAD"
  },
  "jest": {
    "modulePaths": ["./node_modules", "./src/common"]
  }
}
